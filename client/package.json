    "moment-timezone": "^0.5.3",
    "node-sass": "3.4.2",
    "node-uuid": "^1.4.7",
    "postcss-loader": "^0.8.0",
    "react": "15.0.1",
    "react-addons-pure-render-mixin": "15.0.1",
    "react-bootstrap": "^0.28.3",
    "react-cookie": "^0.4.5",
    "react-dom": "15.0.1",
    "react-on-rails": "5.2.0",
    "react-redux": "4.4.5",
    "react-router": "2.4.0",
    "react-router-redux": "^4.0.4",
    "react-star-rating": "^1.4.2",
    "redux": "3.5.2",
    "redux-form": "5.2.2",
    "redux-promise": "0.5.3",
    "redux-thunk": "2.0.1",
    "resolve-url-loader": "^1.4.3",
