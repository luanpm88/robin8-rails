<% if (Robin.china) { %>
  <script type="text/javascript" src="http://maps.google.cn/maps/api/js?libraries=places&sensor=false"></script>
<% } else { %>
  <script type="text/javascript" src="https://maps.google.com/maps/api/js?sensor=false&libraries=places"></script>
<% } %>
<div class="panel panel-default panel-custom">
  <div id="growler-alert"></div>
  <div class="panel-body">
    <div class="row">
      <h3>Edit Profile</h3>
      <hr>
      <!-- left column -->
      <!-- edit form column -->
      <div class="col-md-9 personal-info">
        <h3>Personal info</h3>
        <form class="form-horizontal" role="form" autocomplete="off" id="profileForm" onsubmit="return false;">
          <div class="form-group col-md-6">
            <label class="control-label">First name:</label>
            <div>
              <input class="form-control" name="first_name" type="text" value="">
            </div>
          </div>
          <div class="form-group col-md-6">
            <label class="control-label">Last name:</label>
            <div>
              <input class="form-control" name="last_name" type="text" value="">
            </div>
          </div>
          <div class="form-group col-md-6">
            <label class="control-label">Location:</label>
            <div>
              <input class="form-control" name="location" id="location" type="text" value="">
            </div>
          </div>
          <div class="form-group col-md-6 pull-right">
            <label class="control-label">Email:</label>
            <div>
              <input class="form-control" name="email" type="text" value="" readonly="readonly">
            </div>
          </div>
          <div class="form-group col-md-6">
            <label class="control-label">Day of birth:</label>
            <div>
              <input class="form-control" placeholder="Pick a date!" name = "date_of_birthday" id="date_of_birthday" value="">
            </div>
          </div>
          <div class="form-group col-md-6 pull-right">
            <label class="control-label">Mobile number:</label>
            <div>
              <input class="form-control" name="mobile_number" type="number" value="" >
            </div>
          </div>
          <div class="form-group col-md-12">
            <label class="control-label">Title:</label>
            <div>
              <input class="form-control" name="title" type="text" value="">
            </div>
          </div>
          <div class="form-group col-md-12">
            <label class="control-label">Industry:</label>
            <div>
              <input type="text" name="industry" class="form-control" value="" />
            </div>
          </div>
           <div class="form-group col-md-12">
            <label class="control-label">Interests:</label>
            <div>
              <input type="hidden" id="interests" class="form-control" value="Interests" />
            </div>
          </div>
          <hr>
           <div class="form-group col-md-12" id="currentPassword">
            <label class="control-label">Current password:</label>
            <div>
              <input class="form-control" name="current_password" type="password" value="">
            </div>
          </div>
          <div class="form-group col-md-12">
            <label class="control-label">New password:</label>
            <div>
              <input class="form-control" name="password" type="password" value="" minlength="8"
               data-fv-stringlength-message="A new password must be at least 8 characters long">
            </div>
          </div>
          <div class="form-group col-md-12">
            <label class="control-label">Confirm new password:</label>
            <div>
              <input class="form-control" name="password_confirmation" type="password" value="">
            </div>
          </div>
          <div class="form-group col-md-12">
            <input type="checkbox" class='is_public-checkbox' id="is_public" name="is_public">
            <label for="is_public" class="control-label">Public account</label>
          </div>
          <div class="form-group col-md-12">
            <input type="button" id="saveChanges" class="btn btn-primary" value="Save Changes">
            <span></span>
            <a class="btn btn-default" href="/#dashboard">Cancel</a>
            <span></span>
            <input type="button" class="btn btn-primary pull-right" value="Set Up Payoneer payments">
          </div>
        </form>
      </div>
    </div>
  </div>
</div>
