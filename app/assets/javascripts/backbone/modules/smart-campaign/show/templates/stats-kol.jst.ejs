<div class="row campaigns-list-table">
  <table class="table table-striped table-bordered dataTable" cellspacing="0" width="100%" id="campaigns-table22">
      <thead style="cursor:pointer;">
      <tr>
        <th>KOL名称</th>
        <th>有效点击</th>
        <th>总点击</th>
        <th>接收时间</th>
      </tr>
      </thead>
    <tbody>
    <% _.each(items, function(c) { %>
    <tr data-id="<%= c.id %>">
      <td class="middle col-md-1"><%= c.kol.first_name %> &nbsp; &nbsp; <%= c.kol.last_name %></td>
      <td class="middle col-md-1"><%= c.get_avail_click %></td>
      <td class="middle col-md-1"><%= c.get_total_click %></td>
      <td class="middle col-md-1"><%= formatDate(c.approved_at) %></td>
    </tr>
    <% }); %>
    </tbody>
  </table>
</div>
