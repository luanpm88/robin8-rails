<div class="panel-heading">
  <h1 class="panel-title"><i class="fa fa-search"></i><strong> Search contacts database</strong></h1>
</div>
<div class="panel-body">
  <form role="form" class="form">
    <div class="form-group">
      <label>Keywords</label>
      <input type="text" autocomplete="off" placeholder="Enter keywords" class="typeahead form-control">
    </div>
    <div class="form-group">
      <label>Contact name</label>
      <input type="text" autocomplete="off" placeholder="Contact's first and last name" class="typeahead form-control">
    </div>
    <div class="form-group">
      <label>Outlet</label>
      <input type="text" autocomplete="off" placeholder="Outlet website or blog name" class="typeahead form-control">
    </div>
    <div class="form-group form-inline">
      <label>Location</label>
      <br>
      <input type="text" autocomplete="off" placeholder="City" class="typeahead form-control">
      <input type="text" autocomplete="off" placeholder="State/Province" class="typeahead form-control">
      <input type="text" autocomplete="off" placeholder="Country" class="typeahead form-control">
    </div>
    <button class="btn btn-success pull-right btn-small" type="submit" id="social-search">Search</button>
  </form>
</div>
