<li role="presentation" class="<%= (level === 1) ? 'active' : '' %> <%= (level >= 1) ? 'colored' : '' %>">
  <a id="blast-home-link" href="#blast-home" aria-controls="home" role="tab">
    <i class="fa fa-home"></i> Start 
    <% if (Robin.user.get('smart_release_available_count') == undefined) {%>
      <label class="badge badge-success badge-tooltip"><i style="font-size: 10px;" class="fa fa-refresh fa-spin"></i> </label>
    <% } else { %>
      <label class="badge badge-success badge-tooltip"><%= Robin.user.get('smart_release_available_count') %> </label>
    <% } %>
  </a>
</li>


<li role="presentation" class="<%= (level === 2) ? 'active' : '' %> <%= (level >= 2) ? 'colored' : '' %>"><a id="blast-analysis-link" href="#blast-analysis" aria-controls="profile" role="tab"><i class="fa fa-bar-chart"></i> Analysis</a></li>
<li role="presentation" class="<%= (level === 3) ? 'active' : '' %> <%= (level >= 3) ? 'colored' : '' %>"><a id="blast-targets-link" href="#blast-targets" aria-controls="messages" role="tab"><i class="fa fa-bullseye"></i> Targets</a></li>
<li role="presentation" class="<%= (level === 4) ? 'active' : '' %> <%= (level >= 4) ? 'colored' : '' %>"><a id="blast-pitch-link" href="#blast-pitch" aria-controls="settings" role="tab"><i class="fa fa-send-o"></i> Pitch <label class="badge badge-success"><%= pitchContactsCollection.length %></label></a></li>
