<div class="clearfix">
  <img class="discover-item-img" alt="">
  <div class="discover-w">
    <div class="discover-item-header clearfix">
      <h5><%= name %></h5>
      <p>
        <% var date = new Date(start_time) %>
        <i class='glyphicon glyphicon-time'></i> <%= date.getMonth() %>月<%= date.getDate() %>日 &nbsp;&nbsp;
        <i class='glyphicon glyphicon-piggy-bank'></i> 当前收入： <%= avail_click * per_click_budget %> &nbsp;&nbsp;
        <a href="#" class='cam-item btn btn-primary' id='<%= id %>'>
          <i class='glyphicon glyphicon-search'></i>&nbsp;查看
        </a>
      </p>
    </div>
    <div class="discover-item-body">
      <p>
        <%= description %>
      </p>
    </div>

  </div>

</div>
