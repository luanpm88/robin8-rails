<!DOCTYPE html>
<html>
  <head>
    <title>Robin8 Marketing Dashboard</title>
    <meta charset='utf-8' />
    <%= stylesheet_link_tag 'admin' %>
    <% if (params[:controller].include? 'sessions'  rescue false)  %>
    <% else %>
      <%= javascript_include_tag 'admin' %>
      <%= javascript_include_tag '//cdn.staticfile.org/Plupload/2.1.1/plupload.full.min.js' %>
      <%= javascript_include_tag '//cdn.staticfile.org/qiniu-js-sdk/1.0.14-beta/qiniu.min.js' %>
    <% end %>
    <%= csrf_meta_tag %>
    <%= favicon_link_tag 'favicon.ico' %>
  </head>
  <body>
      <div id="wrapper">
        <!-- Navigation -->
        <%= render :partial => "layouts/partners_nav" %>

        <div id="page-wrapper">
          <%= yield %>
        </div>
        <!-- /#page-wrapper -->
      </div>
      <!-- /#wrapper -->
  </body>
</html>
