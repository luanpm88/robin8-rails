<html>
  <head>
    <meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
    <meta http-equiv="pragma" content="no-cache">
    <meta name="format-detection" content="telephone=no">
    <meta name="viewport" content="width=device-width,initial-scale=1.0,maximum-scale=1.0,user-scalable=0">
    <title>
      好用不贵的14支韩系口红
    </title>

    
    <%= stylesheet_link_tag    'cpses/app-webview-v2', media: 'all', 'data-turbolinks-track' => true %>
  </head>
  <body class="">
    <div class="wrapper" style="padding-top: 110px;">
      <div class="inner">
        <div class="banner-space">
        </div>

        <%= render "carticles/header_partial" %>
        <% Carticle.last.get_formated_content.each do |item| %>
          <% if item[0] == "text" %>
            <%= render "carticles/text_partial", :text => item[1] %>
          <% end %>
          <% if item[0] == "img" %>
            <%= render "carticles/image_partial", :image => item[1] %>
          <% end %>
          <% if item[0] == "product" %>
            <%= render "carticles/purchase_partial", :product_id => item[1] %>
          <% end %>
        <% end %>
      </div>
    </div>
    <script type="text/javascript">
      function nightMode() {
        var body = document.getElementsByTagName("body")[0];
        body.className = body.className + " night-mode";
      };
    </script>
    </div>
  </body>
</html>