<table class="table table-bordered">
  <thead>
    <tr>
      <th>ID</th>
      <th>Campaign ID</th>
      <th>Kol ID</th>
      <th>Cookies</th>
      <th>IP</th>
      <th>Time</th>
      <th>Status</th>
      <th>Agent</th>
      <th>Referer</th>
    </tr>
  </thead>
  <tbody>
    <% @campaign_shows.each do |c| %>
      <tr>
        <% %w(id campaign_id kol_id visitor_cookie visitor_ip visit_time status visitor_agent visitor_referer).each do |v| %>
          <td><%= c.send v %></td>
        <% end %>
      </tr>

    <% end %>
  </tbody>
</table>

<%= will_paginate @campaign_shows %>
