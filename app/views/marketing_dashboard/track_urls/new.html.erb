<div class="row">
  <div class="col-md-4 col-md-offset-4">
    <div class="login-panel panel panel-default">
      <div class="panel-heading">
        <h3 class="panel-title">新建追踪链接</h3>
      </div>
      <div class="panel-body">
        <%= form_for(@track_url, as: :track_url, url: marketing_dashboard_track_urls_path(@track_url), method: :post) do |f| %>
          <div class="field form-group">
            <%= devise_error_messages! %>
          </div>
          <div class="field form-group">
            <%= f.label "原链接" %><br />
            <%= f.text_field :origin_url, autofocus: true, class: "form-control" %>
          </div>
          
          <div class="field form-group">
            <%= f.label "备注" %><br />
            <%= f.text_field :desc, autofocus: true, class: "form-control" %>
          </div>
          <div class="actions">
            <%= f.submit "保存", class: "btn btn-lg btn-success btn-block" %>
          </div>
        <% end %>
      </div>
    </div>
  </div>
</div>
