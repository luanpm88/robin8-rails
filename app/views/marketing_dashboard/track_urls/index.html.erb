<div class="row">
  <h5><%= devise_error_messages! %></h5>
  <div class="well">
    <%= link_to "新建追踪链接", new_marketing_dashboard_track_url_path, class: "btn btn-primary pull-right" %>
  </div>
  <div class="panel panel-default">

    <div class="panel-body">
      <div class="table-responsive">
        <table class="table table-striped table-bordered table-hover">
          <thead>
            <tr>
              <th>原链接</th>
              <th>短链</th>
              <th>备注</th>
              <th>点击数</th>
              <th>修改</th>
            </tr>
          </thead>
          <tbody>
            <% @track_urls.each do |url| %>
              <tr>
                <td><%= url.origin_url %> </td>
                <td><%= link_to url.short_url_text, url.short_url_text, :target => "_blank" %> </td>
                <td><%= url.desc %> </td>
                <td><%= url.click_count %> </td>
                <td><%= link_to "修改", edit_marketing_dashboard_track_url_path(url)%></td>
              </tr>
            <% end %>
          </tbody>
        </table>
      </div>
    </div>
  </div>
</div>
<%= will_paginate @track_urls %>