<div id="page-wrapper">
  <div class="row">
    <div class="col-lg-6">
      <h5><%= devise_error_messages! %></h5>
      <div class="panel panel-default">
        <div class="panel-heading">
           追踪链接列表
           <% if current_account.is_admin? %>
             <%= "" #link_to "新建管理员", new_marketing_dashboard_path, class: "btn btn-primary pull-right" %>
           <% end %>
        </div>
        <div class="panel-body">
          <div class="table-responsive">
            <table class="table table-striped table-bordered table-hover">
              <thead>
                <tr>
                  <th>原链接</th>
                  <th>短链</th>
                  <th>备注</th>
                  <th>点击数</th>
                </tr>
              </thead>
              <tbody>
                <% @track_urls.each do |url| %>
                <tr>
                  <td><%= url.origin_url %> </td>
                  <td><%= account.short_url %> </td>
                  <td><%= account.desc %> </td>
                  <td><%= account.click_count %> </td>
                </tr>
                <% end %>
              </tbody>
            </table>
          </div>
        </div>
      </div>
    </div>
  </div>
</div>