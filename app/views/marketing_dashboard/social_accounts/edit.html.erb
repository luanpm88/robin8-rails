<div class="container-fluid">
  <h2>
    编辑社交帐号
    <%= link_to "返回上一页", marketing_dashboard_social_accounts_path(:kol_id => params[:kol_id]), class: "btn btn-default pull-right" %>
  </h2>

  <%= form_for([:marketing_dashboard, @social_account], url: marketing_dashboard_social_account_path(:kol_id => params[:kol_id], :id => @social_account.id), method: :put, html: { multiple: true}) do |f| %>
    <%= render partial: 'form', locals: { f: f}  %>
  <% end %>
</div>
