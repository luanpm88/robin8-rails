<% if @tags.present? %>
  <div style="margin-left: 20px">
  <p>Add tag</p>
  <%= form_tag marketing_dashboard_circle_add_tag_path do %>
    <% @tags.each do |tag| %>
      <%= check_box_tag 'tag_ids[]', tag.id,  @circle.tags.find_by_id(tag.id)? true : false -%>
      <%= h tag.name -%>
    <% end %><br/>
    <%= submit_tag 'submit', :class => 'btn btn-primary' %>
  <% end %>
  </div>
<% else %>
  <%= link_to "æ·»åŠ Tag", "#"%>
<% end %>

