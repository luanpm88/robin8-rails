<table class='table table-bordered'>
  <thead>
    <tr>
      <th>ID</th>
      <th>App Version</th>
      <th>Platform</th>
      <th>OS Version</th>
      <th>Device Model</th>
      <th>Content</th>
      <th>Screenshot</th>
    </tr>
  </thead>

  <tbody>
    <% @feedbacks.each do |f| %>
      <tr>
        <% %w(id app_version app_platform os_version device_model content).each do |v| %>
          <td> <%= f.send v %> </td>
        <% end %>
        <td> <img src="<%= f.screenshot.url %>" alt=""> </td>
      </tr>
    <% end %>
  </tbody>

</table>

<%= will_paginate @feedbacks %>
